0000                  00019         ORG     0
0000                  00020 Inicio
0000   28FE           00021         goto    Principal               ; Posición 000h de memoria de programa.
0001   0103           00022         clrw                            ; Posición 001h de memoria de programa.
0002   2800           00023         goto    Inicio                  ; Posición 002h de memoria de programa.
                      00024 
00FE                  00025         ORG     0xFE                    ; Fija la posición de la memoria de programa en 0x0FE.
00FE                  00026 Principal
00FE   3001           00027         movlw   .1                      ; Posición 0FEh de memoria de programa.
00FF   0782           00028         addwf   PCL,F                   ; Posición 0FFh de memoria de programa.
0100   2902           00029         goto    Configuracion0          ; Posición 100h de memoria de programa.
0101   2902           00030         goto    Configuracion1          ; Posición 101h de memoria de programa.
                      00031         
                      00032 ; La intención de la instrucción "addwf PCL,F" es saltar a la posición de la instrucción 
                      00033 ; "goto Configuracion1", que está en la posición 101h de memoria de programa pero, como el
                      00034 ; contenido del registro PCLATH no ha cambiado de valor, realmente salta a la posición que 
                      00035 ; indica el contador de programa que en este caso es (PC)=(PCLATH)(PCL) = 0001h, es decir, ha 
                      00036 ; saltado a la posición donde se encuentra la instrucción "clrw". El salto se ha descontrolado.
                      00037 
0102                  00038 Configuracion0
0102                  00039 Configuracion1
                      00040 
