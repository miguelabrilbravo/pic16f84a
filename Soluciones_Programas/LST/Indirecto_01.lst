                      00008 ;
                      00009 ; Este programa comprueba el funcionamiento del direccionamiento indirecto. Se trata de
                      00010 ; escribir con el valor de una constante a partir de la última dirección ocupada de
                      00011 ; la memoria RAM de datos hasta el final.
                      00012 ; Su correcto funcionamiento debe comprobarse con el simulador del MPLAB.
                      00013 ;
                      00014 ; ZONA DE DATOS **********************************************************************
                      00015 
2007   3FF1           00016         __CONFIG   _CP_OFF  &  _WDT_OFF & _PWRTE_ON & _XT_OSC
                      00017         LIST       P=16F84A
                      00018         INCLUDE  <P16F84A.INC>
                      00001         LIST
                      00002 ; P16F84A.INC  Standard Header File, Version 2.00    Microchip Technology, Inc.
                      00134         LIST
                      00019 
                      00020         CBLOCK  0x0C
  0000000C            00021         RAM_Contador
                      00022         ENDC
                      00023 
  0000002A            00024 Constante               EQU     2Ah             ; Por ejemplo.
  0000004F            00025 RAM_UltimaDireccion  EQU        4Fh             ; Ultima dirección de RAM de datos utilizada para
                      00026                                         ; el PIC16F84A.
                      00027 ; ZONA DE CÓDIGOS ********************************************************************
                      00028 
0000                  00029         ORG     0
0000                  00030 Inicio
0000   3043           00031         movlw   RAM_UltimaDireccion-RAM_Contador        ; Número de posiciones a escribir.
0001   008C           00032         movwf   RAM_Contador
0002   300D           00033         movlw   RAM_Contador+1          ; Primera posición de RAM libre.
0003   0084           00034         movwf   FSR                     ; Primera dirección de memoria RAM a escribir
0004                  00035 RAM_EscribeConstante
0004   302A           00036         movlw   Constante               ; Escribe el valor de la constante en la posición
0005   0080           00037         movwf   INDF                    ; apuntada por FSR. (W) -> ((FSR))
0006   0A84           00038         incf    FSR,F                   ; Apunta a la siguiente dirección de memoria.
0007   0B8C           00039         decfsz  RAM_Contador,F
0008   2804           00040         goto    RAM_EscribeConstante
0009                  00041 Principal
0009   0063           00042         sleep                           ; Pasa a reposo.
                      00043 
                      00044         END
