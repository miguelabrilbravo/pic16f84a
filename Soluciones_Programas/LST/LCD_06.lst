0067                  00179 LCD_EnheMinuscula                       ; un carácter especial.
0067   3CF1           00180         sublw   'ñ'                     ; ¿Es la "ñ"?
0068   1D03           00181         btfss   STATUS,Z
0069   286D           00182         goto    LCD_EnheMayuscula       ; No es "ñ".
