                      00008 ;
                      00009 ; Un número binario natural de 8 bits es convertido a BCD. El resultado se guarda en tres
                      00010 ; posiciones de memorias llamadas: BCD_Centenas, BCD_Decenas y BCD_Unidades.
                      00011 ;
                      00012 ; El procedimiento utilizado es mediante restas de 10 tal como se explicó en el capítulo 9.
                      00013 ;
                      00014 ; Entrada:      En el registro W el número binario natural a convertir.
                      00015 ; Salidas:      En (BCD_Centenas), (BCD_Decenas) y (BCD_Unidades).
                      00016 ;               En el registro W también las decenas (nibble alto) y unidades (nibble bajo).
                      00017 
                      00018 ; Subrutina "BIN_a_BCD" -----------------------------------------------------------------
                      00019 
                      00020         CBLOCK                          ; En las subrutinas no se debe fijar la dirección
  00000010            00021         BCD_Centenas                    ; de la RAM de usuario. Se toma a continuación de
  00000011            00022         BCD_Decenas                     ; la última asignada.
  00000012            00023         BCD_Unidades    
                      00024         ENDC
                      00025 ;
0065                  00026 BIN_a_BCD
0065   0190           00027         clrf    BCD_Centenas            ; Carga los registros con el resultado inicial.
0066   0191           00028         clrf    BCD_Decenas             ; En principio las centenas y decenas a cero.
0067   0092           00029         movwf   BCD_Unidades            ; Se carga el número binario a convertir.
0068                  00030 BCD_Resta10
0068   300A           00031         movlw   .10                     ; A las unidades se le va restando 10 en cada
0069   0212           00032         subwf   BCD_Unidades,W          ; pasada. (W)=(BCD_Unidades) -10.
006A   1C03           00033         btfss   STATUS,C                ; ¿C = 1?, ¿(W) positivo?, ¿(BCD_Unidades)>=10?.
006B   2875           00034         goto    BIN_BCD_Fin             ; No, es menor de 10. Se acabó.
006C                  00035 BCD_IncrementaDecenas
006C   0092           00036         movwf   BCD_Unidades            ; Recupera lo que queda por restar.
006D   0A91           00037         incf    BCD_Decenas,F           ; Incrementa las decenas y comprueba si ha llegado
006E   300A           00038         movlw   .10                     ; a 10. Lo hace mediante una resta.
006F   0211           00039         subwf   BCD_Decenas,W           ; (W)=(BCD_Decenas)-10).
