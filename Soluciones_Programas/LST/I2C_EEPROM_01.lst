                      00129 ;
                      00130 ; El microcontrolador maestro lee un byte desde el esclavo conectado al bus I2C. El dato
                      00131 ; recibido se carga en el registro I2C_Dato y lo envía a la subrutina superior a través
                      00132 ; del registro W. Se empieza a leer por el bit de mayor peso MSB.
                      00133 ; De alguna de las subrutinas ejecutadas anteriormente I2C_EnviaStart, I2C_EnviaByte
                      00134 ; o esta misma I2C_LeeByte, la línea SCL lleva en bajo al menos 5 µs.
                      00135 
005E                  00136 I2C_LeeByte
005E   3008           00137         movlw   0x08                    ; A recibir 8 bits.
005F   008D           00138         movwf   I2C_ContadorBits
0060   202C           00139         call    SDA_AltaImpedancia      ; Deja libre la línea de datos.
0061                  00140 I2C_LeeBit
0061   2035           00141         call    SCL_AltaImpedancia      ; Flanco de subida del reloj.
0062   1003           00142         bcf     STATUS,C                ; En principio supone que es "0".
0063   1A05           00143         btfsc   SDA                     ; Lee el bit
0064   1403           00144         bsf     STATUS,C                ; Si es "1" carga 1 en el Carry.
0065   0D8E           00145         rlf     I2C_Dato,F              ; Lo introduce en el registro.
0066   2030           00146         call    SCL_Bajo                ; Termina el semiperiodo positivo del reloj.
0067   20B7           00147         call    Retardo_4micros         ; Tiempo tHD;DAT del protocolo.
0068   0B8D           00148         decfsz  I2C_ContadorBits,F              ; Lazo para los 8 bits.
0069   2861           00149         goto    I2C_LeeBit
                      00150 ;
                      00151 ; Chequea si este es el último byte a leer para enviar o no el bit de reconocimiento
                      00152 ; ACK en consecuencia.
                      00153 ;
006A   1C0F           00154         btfss   I2C_UltimoByteLeer      ; Si es el último, no debe enviar
                      00155                                         ; el bit de reconocimiento ACK.
006B   2027           00156         call    SDA_Bajo                ; Envía el bit de reconocimiento ACK
                      00157                                         ; porque todavía no es el último byte a leer.
006C   2035           00158         call    SCL_AltaImpedancia      ; Pulso en alto del SCL para transmitir el
006D   20B7           00159         call    Retardo_4micros         ; bit ACK de reconocimiento. Este es tHIGH.
006E   2030           00160         call    SCL_Bajo                ; Pulso de bajada del SCL.
006F   20B7           00161         call    Retardo_4micros
0070   080E           00162         movf    I2C_Dato,W              ; El resultado se manda en el registro de
0071   0008           00163         return                          ; de trabajo W.
