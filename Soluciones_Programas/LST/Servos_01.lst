                      00020 ;               FactorAlto      0,1·FactorAlto  0,1·(200-FactorAlto)            (Grados)
                      00021 ;               ------------            --------------  ----------------------          ---------
                      00022 ;                  3                0,3 ms                 19,7 ms                        0º
                      00023 ;                  12               1,2 ms                  18,8 ms                      90º
                      00024 ;                  21               2,1 ms                  17,9 ms                     180º
                      00025 ;                  12               1,2 ms                  18,8 ms                      90º
                      00026 ;
