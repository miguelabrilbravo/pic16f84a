000D   280F           00051         goto    Retardos_ms             ; Aporta 2 ciclos máquina.
000E                  00052 Retardo_1ms                             ; La llamada "call ..." aporta 2 ciclos máquina.
000E   3001           00053         movlw   d'1'                    ; Aporta 1 ciclo máquina. Este es el valor de "M".
                      00054 ;
                      00055 ; El próximo bloque "Retardos_ms" tarda:
                      00056 ; 1 + M + M + KxM + (K-1)xM + Mx2 + (K-1)Mx2 + (M-1) + 2 + (M-1)x2 + 2 =
                      00057 ; = (2 + 4M + 4KM) ciclos máquina. Para K=249 y M=1 supone 1002 ciclos máquina
                      00058 ; que a 4 MHz son 1002 µs = 1 ms.
                      00059 ;
000F                  00060 Retardos_ms
000F   008D           00061         movwf   R_ContB                 ; Aporta 1 ciclo máquina.
0010                  00062 R1ms_BucleExterno
0010   30F9           00063         movlw   d'249'                  ; Aporta Mx1 ciclos máquina. Este es el valor de "K".
0011   008C           00064         movwf   R_ContA                 ; Aporta Mx1 ciclos máquina.
0012                  00065 R1ms_BucleInterno
0012   0000           00066         nop                             ; Aporta KxMx1 ciclos máquina.
0013   0B8C           00067         decfsz  R_ContA,F               ; (K-1)xMx1 cm (cuando no salta) + Mx2 cm (al saltar).
0014   2812           00068         goto    R1ms_BucleInterno               ; Aporta (K-1)xMx2 ciclos máquina.
0015   0B8D           00069         decfsz  R_ContB,F               ; (M-1)x1 cm (cuando no salta) + 2 cm (al saltar).
0016   2810           00070         goto    R1ms_BucleExterno       ; Aporta (M-1)x2 ciclos máquina.
0017   0008           00071         return                          ; El salto de retorno aporta 2 ciclos máquina.
                      00072 ;
                      00073 ;En total estas subrutinas tardan:
                      00074 ; - Retardo_200ms:      2 + 1 + 2 + (2 + 4M + 4KM) = 200007 cm = 200 ms. (M=200 y K=249).
                      00075 ; - Retardo_100ms:      2 + 1 + 2 + (2 + 4M + 4KM) = 100007 cm = 100 ms. (M=100 y K=249).
                      00076 ; - Retardo_1ms  :      2 + 1     + (2 + 4M + 4KM) =   1005 cm =   1 ms. (M=  1 y K=249).
                      00077 
