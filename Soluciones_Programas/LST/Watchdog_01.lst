                      00018 ; Subrutina "BIN_a_BCD" -----------------------------------------------------------------
                      00019 
                      00020         CBLOCK                          ; En las subrutinas no se debe fijar la dirección
  00000010            00021         BCD_Centenas                    ; de la RAM de usuario. Se toma a continuación de
  00000011            00022         BCD_Decenas                     ; la última asignada.
  00000012            00023         BCD_Unidades    
                      00024         ENDC
                      00025 ;
0054                  00026 BIN_a_BCD
0054   0190           00027         clrf    BCD_Centenas            ; Carga los registros con el resultado inicial.
0055   0191           00028         clrf    BCD_Decenas             ; En principio las centenas y decenas a cero.
0056   0092           00029         movwf   BCD_Unidades            ; Se carga el número binario a convertir.
0057                  00030 BCD_Resta10
0057   300A           00031         movlw   .10                     ; A las unidades se le va restando 10 en cada
0058   0212           00032         subwf   BCD_Unidades,W          ; pasada. (W)=(BCD_Unidades) -10.
0059   1C03           00033         btfss   STATUS,C                ; ¿C = 1?, ¿(W) positivo?, ¿(BCD_Unidades)>=10?.
005A   2864           00034         goto    BIN_BCD_Fin             ; No, es menor de 10. Se acabó.
005B                  00035 BCD_IncrementaDecenas
005B   0092           00036         movwf   BCD_Unidades            ; Recupera lo que queda por restar.
005C   0A91           00037         incf    BCD_Decenas,F           ; Incrementa las decenas y comprueba si ha llegado
005D   300A           00038         movlw   .10                     ; a 10. Lo hace mediante una resta.
005E   0211           00039         subwf   BCD_Decenas,W           ; (W)=(BCD_Decenas)-10).
005F   1C03           00040         btfss   STATUS,C                ; ¿C = 1?, ¿(W) positivo?, ¿(BCD_Decenas)>=10?.
0060   2857           00041         goto    BCD_Resta10             ; No. Vuelve a dar otra pasada, restándole 10 a
0061                  00042 BCD_IncrementaCentenas                  ; las unidades.
0061   0191           00043         clrf    BCD_Decenas             ; Pone a cero las decenas 
0062   0A90           00044         incf    BCD_Centenas,F          ; e incrementa las centenas.
0063   2857           00045         goto    BCD_Resta10             ; Otra pasada: Resta 10 al número a convertir.
0064                  00046 BIN_BCD_Fin
0064   0E11           00047         swapf   BCD_Decenas,W           ; En el nibble alto de (W) también las decenas.
0065   0712           00048         addwf   BCD_Unidades,W          ; En el nibble bajo de (W) las unidades.
0066   0008           00049         return                          ; Vuelve al programa principal.
