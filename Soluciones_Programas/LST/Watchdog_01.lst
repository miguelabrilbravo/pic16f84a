008A   0813           00096         movf    LCD_Dato,W              ; Recupera el dato a enviar.
008B   0086           00097         movwf   LCD_BusDatos            ; Envía el dato al módulo LCD.
008C   1505           00098         bsf     LCD_PinEnable           ; Permite funcionamiento del LCD, mediante un pequeño
008D   1105           00099         bcf     LCD_PinEnable           ; pulso y termina impidiendo el funcionamiento del LCD.
008E   1C05           00100         btfss   LCD_PinRS               ; Debe garantizar una correcta escritura manteniendo 
